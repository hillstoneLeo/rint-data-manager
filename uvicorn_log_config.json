{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "default": {
      "()": "uvicorn.logging.DefaultFormatter",
      "fmt": "%(levelprefix)s %(message)s",
      "datefmt": "%Y-%m-%d %H:%M:%S"
    },
    "access": {
      "()": "uvicorn.logging.AccessFormatter",
      "fmt": "%(levelprefix)s %(client_addr)s - \"%(request_line)s\" %(status_code)s",
      "datefmt": "%Y-%m-%d %H:%M:%S"
    }
  },
  "handlers": {
    "uvicorn_file": {
      "()": "logging.handlers.RotatingFileHandler",
      "filename": "log/uvicorn.log",
      "maxBytes": 209715200,
      "backupCount": 5,
      "formatter": "default",
      "encoding": "utf-8"
    },
    "access_file": {
      "()": "logging.handlers.RotatingFileHandler",
      "filename": "log/uvicorn.log",
      "maxBytes": 209715200,
      "backupCount": 5,
      "formatter": "access",
      "encoding": "utf-8"
    }
  },
  "loggers": {
    "uvicorn": {
      "handlers": ["uvicorn_file"],
      "level": "INFO",
      "propagate": false
    },
    "uvicorn.access": {
      "handlers": ["access_file"],
      "level": "INFO",
      "propagate": false
    },
    "uvicorn.error": {
      "handlers": ["uvicorn_file"],
      "level": "INFO",
      "propagate": false
    }
  },
  "root": {
    "level": "INFO",
    "handlers": ["uvicorn_file"]
  }
}